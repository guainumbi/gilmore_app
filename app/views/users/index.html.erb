<div class="container">
  <%= link_to 'New Goldfish', new_user_path %>
</div>

  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.name %> <%= user.email %>
        <%= link_to 'Delete', user_path(user), method: :delete %>
        <%= link_to 'Edit', edit_user_path(user) %>
        <%= link_to 'Show', user_path(user) %>
      </li>
    <% end %>
  </ul>
